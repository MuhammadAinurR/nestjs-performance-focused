{
  "reporter": ["text", "html", "lcov", "json"],
  "exclude": [
    "**/*.spec.ts",
    "**/*.e2e-spec.ts",
    "**/test/**",
    "**/node_modules/**",
    "**/dist/**",
    "src/main.ts",
    "**/*.dto.ts",
    "**/*.interface.ts"
  ],
  "all": true,
  "src": "src",
  "clean": true,
  "check-coverage": false,
  "statements": 80,
  "branches": 70,
  "functions": 80,
  "lines": 80
}
